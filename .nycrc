{
	"lines": 100,
	"statements": 100,
	"functions": 100,
	"branches": 100,
	"include": [
		"src/**/*.js"
	],
	"exclude": [
	],
	"reporter": [
		"json",
		"text-summary",
		"html"
	],
	"all": true,
	"check-coverage": true,
	"produce-source-map": true,
	"report-dir": "./coverage",
	"require": [
		"babel-register"
	],
	"sourceMap": false,
	"instrument": false
}
